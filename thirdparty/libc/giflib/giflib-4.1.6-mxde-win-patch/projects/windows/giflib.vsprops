<?xml version="1.0" encoding="Windows-1252"?>
<!--===============================================-->
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="giflib"
	>

    <!--===============================================-->
    
    <UserMacro
		Name="SOURCE_ROOT_DIR"
		Value="../../../../../.."
	/>

    <UserMacro
		Name="SOURCE_BUILD_DIR"
		Value="$(SOURCE_ROOT_DIR)"
	/>

    <!--===============================================-->

    <UserMacro
		Name="GIFLIB_GROUP"
		Value="giflib"
	/>
    <UserMacro
		Name="GIFLIB_NAME"
		Value="giflib"
	/>
    <UserMacro
		Name="GIFLIB_VERSION"
		Value="4.1.6"
	/>
    <UserMacro
		Name="GIFLIB_DIR"
		Value="$(GIFLIB_GROUP)/$(GIFLIB_NAME)-$(GIFLIB_VERSION)"
	/>
    <UserMacro
		Name="GIFLIB_SOURCE_DIR"
		Value="$(SOURCE_ROOT_DIR)/$(GIFLIB_DIR)"
	/>
    <UserMacro
		Name="GIFLIB_BUILD_DIR"
		Value="$(SOURCE_BUILD_DIR)/$(GIFLIB_DIR)"
	/>

    <UserMacro
		Name="GIFLIB_BUILD"
		Value="$(GIFLIB_BUILD_DIR)/build/$(PlatformName)/$(VCVersion)/$(ConfigurationName)"
	/>
    <UserMacro
		Name="GIFLIB_OBJ"
		Value="$(GIFLIB_BUILD)/obj/$(ProjectName)"
	/>
    <UserMacro
		Name="GIFLIB_LIB"
		Value="$(GIFLIB_BUILD)/lib"
	/>
    <UserMacro
		Name="GIFLIB_BIN"
		Value="$(GIFLIB_BUILD)/bin"
	/>
	
    <UserMacro
		Name="GIFLIB_DEFINES"
		Value="HAVE_CONFIG_H"
	/>
    <UserMacro
		Name="GIFLIB_INCLUDE_DIRS"
		Value="$(GIFLIB_SOURCE_DIR)/lib;$(GIFLIB_SOURCE_DIR)"
	/>
    <UserMacro
		Name="GIFLIB_LIB_DIRS"
		Value="$(GIFLIB_LIB)"
	/>
    <UserMacro
		Name="GIFLIB_LIBS"
		Value="libgiflib.lib"
	/>
	
    <!--===============================================-->
    
    <UserMacro
		Name="BUILD_DIR"
		Value="$(GIFLIB_BUILD)"
	/>
    <UserMacro
		Name="BUILD_OBJ"
		Value="$(GIFLIB_OBJ)"
	/>
    <UserMacro
		Name="BUILD_LIB"
		Value="$(GIFLIB_LIB)"
	/>
    <UserMacro
		Name="BUILD_BIN"
		Value="$(GIFLIB_BIN)"
	/>

    <UserMacro
		Name="INCLUDE_DIRS"
		Value="$(GIFLIB_INCLUDE_DIRS)"
	/>
    <UserMacro
		Name="LIB_DIRS"
		Value="$(GIFLIB_LIB_DIRS)"
	/>
    <UserMacro
		Name="LIBS"
		Value=""
	/>
	
    <!--===============================================-->
    
</VisualStudioPropertySheet>
