		Name="DEFINES"
		Value="GLUT_NO_LIB_PRAGMA"
	/>
    <UserMacro
		Name="INCLUDE_DIRS"
		Value="$(GLUT_INCLUDE_DIRS);$(MESA_INCLUDE_DIRS)"
	/>
    <UserMacro
		Name="LIB_DIRS"
		Value="$(GLUT_LIB_DIRS);$(MESA_LIB_DIRS)"
	/>
    <UserMacro
		Name="LIBS"
		Value="$(GLUT_LIBS) $(MESA_LIBS)"
	/>

