<!--==========================================================-->
<develop-project>
    <!--==========================================================-->
    <!-- paths                                                    -->
    <!--==========================================================-->
    <develop-paths id="">
        <develop-path id="template" type="template" href="."/>
        <develop-path id="src" type="src" href="."/>
        <develop-path id="lib" type="lib" href="."/>
        <develop-path id="obj" type="obj" href="."/>
        <develop-path id="bin" type="bin" href="."/>
    </develop-paths>

    <!--==========================================================-->
	<!-- compilers                                                -->
    <!--==========================================================-->
    <develop-compilers id="">
        <!--==========================================================-->
		<!-- compiler GCC Kernel                                      -->
        <!--==========================================================-->
		<develop-compiler id="gcc" type="kernel_c" selected="yes">
			<command>gcc -c</command>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-platforms>
				<develop-platform id="linux" selected="yes"/>
			</develop-platforms>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-path-options id="" type="include" prefix="-I">
				<develop-path-option href="."/>
				<develop-path-option href="/usr/src/linux-2.4/include"/>
			</develop-path-options>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-options id="define" prefix="-D">
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="" selected="yes">
					<value>_DBPRINTK</value>
				</develop-option>
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="" selected="yes">
					<value>__KERNEL__</value>
				</develop-option>
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="" selected="yes">
					<value>LINUX</value>
				</develop-option>
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="" selected="yes">
					<value>MODULE</value>
				</develop-option>
			</develop-options>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-options id="warn" prefix="-W">
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="all" selected="yes">
					<value>all</value>
				</develop-option>
			</develop-options>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-options id="optimize" prefix="-O">
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="level" selected="yes">
					<value>2</value>
				</develop-option>
			</develop-options>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-options id="" prefix="-">
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="pipe" selected="yes">
					<value>pipe</value>
				</develop-option>
			</develop-options>
		</develop-compiler>

        <!--==========================================================-->
		<!-- compiler GCC                                                -->
        <!--==========================================================-->
		<develop-compiler id="gcc" type="c" selected="yes">
			<command>gcc -c</command>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-platforms>
				<develop-platform id="linux" selected="yes"/>
			</develop-platforms>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-path-options id="" type="include" prefix="-I">
				<develop-path-option href="."/>
			</develop-path-options>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-options id="" prefix="-">
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="debug" selected="yes">
					<value>g</value>
				</develop-option>
			</develop-options>
		</develop-compiler>

        <!--==========================================================-->
		<!-- compiler G++                                             -->
        <!--==========================================================-->
		<develop-compiler id="g++" type="cxx" selected="yes">
			<command>g++ -c</command>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-platforms>
				<develop-platform id="linux" selected="yes"/>
			</develop-platforms>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-path-options id="" type="include" prefix="-I">
				<develop-path-option href="."/>
			</develop-path-options>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-options id="" prefix="-">
				<!--==========================================================-->
				<!--                                                          -->
				<!--==========================================================-->
				<develop-option id="debug" selected="yes">
					<value>g</value>
				</develop-option>
			</develop-options>
		</develop-compiler>

    </develop-compilers>

    <!--==========================================================-->
	<!-- linkers                                                  -->
    <!--==========================================================-->
    <develop-linkers id="">
		<!--==========================================================-->
		<!-- linker LD Kernel                                         -->
		<!--==========================================================-->
		<develop-linker id="ld" type="kernel_obj" selected="yes">
			<command>ld -r</command>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-platforms>
				<develop-platform id="linux" selected="yes"/>
			</develop-platforms>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-path-options id="" type="lib" prefix="-L">
				<develop-path-option href="."/>
			</develop-path-options>
		</develop-linker>

		<!--==========================================================-->
		<!-- linker GCC                                               -->
		<!--==========================================================-->
		<develop-linker id="gcc" type="obj" selected="no">
			<command>gcc</command>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-platforms>
				<develop-platform id="linux" selected="yes"/>
			</develop-platforms>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-path-options id="" type="lib" prefix="-L">
				<develop-path-option href="."/>
			</develop-path-options>
		</develop-linker>

		<!--==========================================================-->
		<!-- linker G++                                               -->
		<!--==========================================================-->
		<develop-linker id="g++" type="obj" selected="yes">
			<command>g++</command>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-platforms>
				<develop-platform id="linux" selected="yes"/>
			</develop-platforms>
			<!--==========================================================-->
			<!--                                                          -->
			<!--==========================================================-->
			<develop-path-options id="" type="lib" prefix="-L">
				<develop-path-option href="."/>
			</develop-path-options>
		</develop-linker>
    </develop-linkers>

    <!--==========================================================-->
    <!-- targets                                                  -->
    <!--==========================================================-->
    <develop-targets id="ccl">
        <!--==========================================================-->
        <!-- target                                                   -->
        <!--==========================================================-->
        <develop-target id="socket" type="exe" target="socket">
            <!--==========================================================-->
            <!-- source                                                   -->
            <!--==========================================================-->
            <develop-source id="cmain.cxx" type="cxx" src="$(SRC)/cmain.cxx"/>

            <!--==========================================================-->
            <!-- source                                                   -->
            <!--==========================================================-->
            <develop-source id="socket.cxx" type="cxx" src="$(SRC)/socket.cxx"/>
        </develop-target>
    </develop-targets>
</develop-project>
