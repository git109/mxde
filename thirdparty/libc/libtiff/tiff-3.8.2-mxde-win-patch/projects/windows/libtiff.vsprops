	/>
    <UserMacro
		Name="ZLIB_VERSION"
		Value="1.2.5"
	/>
    <UserMacro
		Name="ZLIB_DIR"
		Value="$(ZLIB_GROUP)/$(ZLIB_NAME)-$(ZLIB_VERSION)"
