		Name="FREEGLUT_GROUP"
		Value="FreeGLUT"
	/>
    <UserMacro
		Name="FREEGLUT_NAME"
		Value="freeglut"
	/>
    <UserMacro
		Name="FREEGLUT_VERSION"
		Value="2.6.0"
	/>
    <UserMacro
		Name="FREEGLUT_GROUP_DIR"
		Value="$(OPENGL_GROUP_DIR)/$(FREEGLUT_GROUP)"
	/>
    <UserMacro
		Name="FREEGLUT_DIR"
		Value="$(FREEGLUT_GROUP_DIR)/$(FREEGLUT_NAME)-$(FREEGLUT_VERSION)"
	/>
    <UserMacro
		Name="FREEGLUT_INCLUDE_DIRS"
		Value="$(FREEGLUT_DIR)/include"
	/>
    <UserMacro
		Name="FREEGLUT_LIB_DIRS"
		Value="$(MESA_LIB_DIRS)"
	/>
    <UserMacro
		Name="FREEGLUT_LIBS"
		Value="freeglut.lib"
	/>

    <!--===============================================-->

    <UserMacro
		Name="GLUT_INCLUDE_DIRS"
		Value="$(MESAGLUT_INCLUDE_DIRS)"
	/>
    <UserMacro
		Name="GLUT_LIB_DIRS"
		Value="$(MESAGLUT_LIB_DIRS)"
	/>
    <UserMacro
		Name="GLUT_LIBS"
		Value="$(MESAGLUT_LIBS)"
	/>

    <!--===============================================-->

    <UserMacro
		Name="BUILD_DIR"
		Value="$(MESA_BUILD_DIR)"
	/>
    <UserMacro
		Name="BUILD_OBJ"
		Value="$(MESA_BUILD_OBJ)"
	/>
    <UserMacro
		Name="BUILD_LIB"
		Value="$(MESA_BUILD_LIB)"
	/>
    <UserMacro
		Name="BUILD_BIN"
		Value="$(MESA_BUILD_BIN)"
	/>

